{
  "name": "@wallstreetsim/db",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema/index.mjs",
      "require": "./dist/schema/index.js",
      "types": "./dist/schema/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/schema/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/schema/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "seed": "tsx src/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@wallstreetsim/types": "workspace:*",
    "@wallstreetsim/utils": "workspace:*",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.0"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
